{
	"name": "react-template",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"axios": "^0.19.0",
		"bcrypt": "^5.0.0",
		"bcrypt-nodejs": "0.0.3",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.17.2",
		"bootstrap": "^4.4.1",
		"cookie-parser": "^1.4.4",
		"dotenv": "^4.0.0",
		"express": "^4.15.3",
		"express-session": "^1.15.4",
		"express-session-sequelize": "^2.1.0",
		"morgan": "^1.8.2",
		"mysql2": "^2.0.2",
		"passport": "^0.3.2",
		"passport-google-oauth": "^2.0.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-local": "^1.0.0",
		"python": "0.0.4",
		"react-bootstrap": "^1.0.0-beta.16",
		"sequelize": "^5.21.3",
		"socket.io": "^2.3.0"
	},
	"devDependencies": {
		"concurrently": "^3.5.0",
		"eslint": "^6.7.2",
		"eslint-plugin-react": "^7.17.0",
		"if-env": "^1.0.4",
		"react-scripts": "^3.3.0"
	},
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:prod",
		"start:prod": "node server.js",
		"start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\"",
		"client": "cd client && npm run start",
		"install": "cd client && npm install",
		"build": "cd client && npm run build",
		"prod": "node server.js",
		"heroku-postbuild": "npm run build",
		"seed": "node utils/scripts/seeds.js"
	},
	"engines": {
		"node": "12.13.0"
	},
	"proxy": {
		"/api/*": {
			"target": "http://localhost:8081",
			"secure": false
		}
	}
}
